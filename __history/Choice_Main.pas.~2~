{$R *.dfm}

uses MainU, MaintananeCU;

procedure TChoice_Maintanance.cxButton2Click(Sender: TObject);
begin
        if  IsFormOpen('MaintananeC') = False  then
            Application.CreateForm(TMaintananeC,MaintananeC);
            MaintananeC.ShowModal;
end;

