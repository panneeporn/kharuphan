  object MyQuery1: TMyQuery
    Connection = DataModule1.gxsoft
    SQL.Strings = (
      
        'select *,if (((NOW() >= h_datebegin ) and (NOW() <= h_dateend)),' +
        #39'd'#39','#39'n'#39')as day from hrd '
      
